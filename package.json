{
  "name": "bridges-server",
  "scripts": {
    "deploy:prod": "sls deploy --stage prod",
    "serve": "sls offline start",
    "build:aws": "sls package",
    "test": "tsx ./src/adapters/test.ts",
    "test-txs": "tsx ./src/utils/testAdapterHistorical.ts",
    "ts": "tsc --noEmit",
    "adapter": "export $(cat .env.test | xargs) &&  tsx  ./src/utils/runAdapterHistorical.ts",
    "aggregate": "export $(cat .env.test | xargs) &&  tsx ./src/utils/testaggregate.ts",
    "daily-volume": "export $(cat .env.test | xargs) &&  tsx ./src/utils/testDailyVolume.ts",
    "dev": "vite",
    "build": "vite build",
    "start": "pm2 start dist/index.js --name bridges-server",
    "start:dev": "pm2 start dist/index.js --name bridges-server --watch",
    "stop": "pm2 stop bridges-server",
    "restart": "pm2 restart bridges-server",
    "logs": "pm2 logs bridges-server",
    "monitor": "pm2 monit bridges-server"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.8",
    "@types/aws-lambda": "^8.10.101",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.19.67",
    "@types/node-fetch": "^2.6.2",
    "@types/retry": "^0.12.5",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "serverless": "^3.21.0",
    "serverless-esbuild": "^1.54.6",
    "serverless-offline": "^12.0.0",
    "serverless-prune-plugin": "^2.0.1",
    "vite": "^6.0.1"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.637.0",
    "@aws-sdk/client-s3": "^3.637.0",
    "@defillama/sdk": "^5.0.94",
    "@fastify/cors": "^9.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@solana/web3.js": "^1.87.3",
    "async-retry": "^1.3.1",
    "axios": "^0.21.0",
    "axios-rate-limit": "^1.3.0",
    "bignumber.js": "^9.0.1",
    "dayjs": "^1.11.13",
    "dotenv": "^8.2.0",
    "ethers": "^5",
    "fastify": "^4.26.2",
    "graphql": "^16.0.0",
    "graphql-request": "^6.1.0",
    "node-fetch": "^2.6.7",
    "postgres": "^3.2.4",
    "tron-format-address": "^0.1.11",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "pm2": "^5.3.1",
    "esbuild": "0.23.1"
  }
}
